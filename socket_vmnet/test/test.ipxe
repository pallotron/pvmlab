#!ipxe
dhcp
echo <socket_vmnet:iPXE-testing>
show version
show mac
show chip
show ip
show gateway
show netmask
show dns
show dhcp-server
echo </socket_vmnet:iPXE-testing>

echo [testing Internet connection]
imgfetch http://google.com/

echo [sleeping for flushing the serial console]
sleep 1

echo [exiting]
reboot
